{
  "name": "terralink-portal-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "dev": "vite --port 6001",
    "dev:all": "concurrently \"npm run dev:fastapi\" \"npm run dev:frontend\"",
    "dev:api": "cd backend && python run_dev.py",
    "dev:fastapi": "cd backend && python run_dev.py",
    "dev:frontend": "vite --port 6001",
    "build": "tsc -b && vite build",
    "vercel-build": "npm run build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "node tests/test-admin-panel.mjs",
    "db:migrate": "cd backend && alembic upgrade head",
    "db:makemigrations": "cd backend && alembic revision --autogenerate -m",
    "db:downgrade": "cd backend && alembic downgrade -1"
  },
  "dependencies": {
    "@react-oauth/google": "^0.12.2",
    "@vercel/kv": "^1.0.1",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "google-auth-library": "^9.4.1",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.542.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
